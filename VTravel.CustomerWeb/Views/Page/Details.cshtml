@model PageViewModel

<div class="container">

    <div class="breadCrumbs">

    </div>


    <div class="row">
        <!-- Highlights -->
        <div class="col-md-12">

            <div class="propertyCaption pt-0">
                <div class="row">
                    <div class="col-md-8 ">
                        <h1 class="h3">@Model.sitePage.title</h1>
                    </div>
                </div>
            </div>

            <div class="formWraper dataHighlights">
                @if (Model.sitePage.content != null)
                {
                    @Html.Raw(Model.sitePage.content)
                }

            </div>

        </div>


    </div>
</div>


    
<script type="text/javascript">$('header').addClass('noneBanner');</script>